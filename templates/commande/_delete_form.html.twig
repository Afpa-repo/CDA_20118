<form method="post" action="{{ path('commande_delete', {'id': commande.comId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ commande.comId) }}">
    <button class="btn">Delete</button>
</form>
